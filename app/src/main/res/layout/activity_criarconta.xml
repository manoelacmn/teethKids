<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/teeth_3_2"
    tools:context=".criarConta">


    <ImageView
        android:id="@+id/image_panel"
        android:layout_width="370dp"
        android:layout_height="696dp"
        android:bottomLeftRadius="30dp"
        android:bottomRightRadius="30dp"
        android:scaleType="centerCrop"
        android:src="@drawable/box_widget"
        android:topLeftRadius="30dp"
        android:topRightRadius="30dp"
        android:textColor="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.706" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/text_title"
        android:layout_width="394dp"
        android:layout_height="101dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:text="Criar conta"
        android:textSize="60sp"
        android:textColor="@color/Azul_escuro"
        app:layout_constraintBottom_toTopOf="@+id/image_panel"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.78"
        tools:layout_editor_absoluteX="-36dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/text_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="E-mail"
        android:textColor="@color/Azul_escuro"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/edit_email"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/image_panel" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_email"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite o E-mail"
        android:textColorHint="@color/teal_700"
        android:textColor="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="@+id/image_panel"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintStart_toStartOf="@+id/image_panel"
        app:layout_constraintTop_toTopOf="@+id/image_panel"
        app:layout_constraintVertical_bias="0.07999998" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/text_senha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Senha"
        android:textSize="24sp"
        android:textColor="@color/Azul_escuro"
        app:layout_constraintBottom_toTopOf="@+id/edit_senha"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="@+id/image_panel" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_senha"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite a senha"
        android:textColorHint="@color/teal_700"
        android:inputType="textPassword"
        android:textColor="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="@+id/image_panel"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintStart_toStartOf="@+id/image_panel"
        app:layout_constraintTop_toTopOf="@+id/image_panel"
        app:layout_constraintVertical_bias="0.24000001" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/text_nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Nome"
        android:textSize="24sp"
        android:textColor="@color/Azul_escuro"
        app:layout_constraintBottom_toTopOf="@+id/edit_nome"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/image_panel" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_nome"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite seu Nome"
        android:textColorHint="@color/teal_700"
        android:textColor="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="@+id/image_panel"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintStart_toStartOf="@+id/image_panel"
        app:layout_constraintTop_toTopOf="@+id/image_panel"
        app:layout_constraintVertical_bias="0.39" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/text_endereco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Endereço"
        android:textSize="24sp"
        android:textColor="@color/Azul_escuro"
        app:layout_constraintBottom_toTopOf="@+id/edit_endereco"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="@+id/image_panel" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_endereco"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite seu Endereço"
        android:textColorHint="@color/teal_700"
        android:textColor="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="@+id/image_panel"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintStart_toStartOf="@+id/image_panel"
        app:layout_constraintTop_toTopOf="@+id/image_panel"
        app:layout_constraintVertical_bias="0.55" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/text_curriculo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Currículo"
        android:textSize="24sp"
        android:textColor="@color/Azul_escuro"
        app:layout_constraintBottom_toTopOf="@+id/edit_curriculo"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="@+id/image_panel" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_curriculo"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite seu Currículo"
        android:textColorHint="@color/teal_700"
        android:textColor="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="@+id/image_panel"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintStart_toStartOf="@+id/image_panel"
        app:layout_constraintTop_toTopOf="@+id/image_panel"
        app:layout_constraintVertical_bias="0.71000004" />

    <ImageView
        android:id="@+id/image_panel_camera"
        android:layout_width="225dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="@+id/image_panel"
        app:layout_constraintEnd_toEndOf="@+id/image_panel"
        app:layout_constraintStart_toStartOf="@+id/image_panel"
        app:layout_constraintTop_toTopOf="@+id/image_panel"
        app:layout_constraintVertical_bias="0.87"
        app:srcCompat="@drawable/box_widget" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_register"
        android:layout_width="189dp"
        android:layout_height="74dp"
        android:layout_marginTop="668dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-black"
        android:text="Criar Conta"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/image_panel" />

    <ImageView
        android:id="@+id/image_circle_camera"
        android:layout_width="69dp"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="@+id/image_panel_camera"
        app:layout_constraintEnd_toEndOf="@+id/image_panel_camera"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="@+id/image_panel_camera"
        app:layout_constraintTop_toTopOf="@+id/image_panel_camera"
        app:srcCompat="@drawable/baseline_circle_24" />

    <ImageView
        android:id="@+id/image_camera"
        android:layout_width="62dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/image_circle_camera"
        app:layout_constraintEnd_toEndOf="@+id/image_panel_camera"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="@+id/image_panel_camera"
        app:layout_constraintTop_toTopOf="@+id/image_circle_camera"
        app:srcCompat="@android:drawable/ic_menu_camera" />

    <TextView
        android:id="@+id/text_camera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Abrir Câmera"
        app:layout_constraintBottom_toBottomOf="@+id/image_panel_camera"
        app:layout_constraintEnd_toEndOf="@+id/image_panel_camera"
        app:layout_constraintHorizontal_bias="0.68"
        app:layout_constraintStart_toStartOf="@+id/image_panel_camera"
        app:layout_constraintTop_toTopOf="@+id/image_panel_camera"
        app:layout_constraintVertical_bias="0.488" />

</androidx.constraintlayout.widget.ConstraintLayout>