<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/teeth_3_2"
    tools:context=".criarConta">


    <ImageView
        android:id="@+id/roundedImageView"
        android:layout_width="370dp"
        android:layout_height="696dp"
        android:background="#fffffff"
        android:bottomLeftRadius="30dp"
        android:bottomRightRadius="30dp"
        android:scaleType="centerCrop"
        android:src="@drawable/box_widget"
        android:topLeftRadius="30dp"
        android:topRightRadius="30dp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="160dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView4"
        android:layout_width="394dp"
        android:layout_height="101dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:text="Criar conta"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="-36dp"
        tools:layout_editor_absoluteY="42dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="E-mail"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="191dp" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/EtEmail_Criar_Conta"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite o E-mail"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="245dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Senha"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="312dp" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/EtSenha_Criar_Conta"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite a senha"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="364dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Nome"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="427dp" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/EtNome_Criar_Conta"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite seu Nome"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="479dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Endereço"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="546dp" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/EtEndereco_Criar_Conta"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite seu Endereço"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="599dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Currículo"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="662dp" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/EtCurriculo_Criar_Conta"
        android:layout_width="344dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-condensed-medium"
        android:hint="Digite seu Currículo"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="716dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="225dp"
        android:layout_height="64dp"
        app:srcCompat="@drawable/box_widget"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="774dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/BtnCriarConta"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:backgroundTint="@color/green_project"
        android:fontFamily="sans-serif-black"
        android:text="Criar Conta"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="835dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="69dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/baseline_circle_24"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="775dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="62dp"
        android:layout_height="42dp"
        app:srcCompat="@android:drawable/ic_menu_camera"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="784dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Abrir Câmera"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="796dp" />

</androidx.constraintlayout.widget.ConstraintLayout>